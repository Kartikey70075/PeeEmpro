<link href="~/assets/css/StyleSheet_Row.css" rel="stylesheet" />
<!-- Bootstrap CSS -->

<style>
    .circles-container {
        display: flex;
        align-items: center; /* Center vertically */
    }

    .disable {
        border-spacing: 10px;
        pointer-events: none;
    }

    .circle-wrapper {
        display: flex;
        align-items: center; /* Center line with circle */
    }

    .blurred {
        filter: blur(4px); /* This will blur the input fields */
        background-color: #f0f0f0; /* Optional: changes the background color to indicate the blur */
    }

    .circle {
        width: 100px;
        height: 50px;
        border-radius: 7%;
        background-color: #4CAF50;
        display: flex;
        justify-content: center;
        align-items: center;
        color: white;
        font-weight: bold;
        position: relative;
        z-index: 1;
    }

    .line {
        height: 5px;
        width: 130px; /* Adjust this width for spacing between circles */
        background-color: black;
        display: inline-block; /* Ensure line aligns with circles */
        margin-left: 5px; /* Space between circle and line */
    }
</style>

